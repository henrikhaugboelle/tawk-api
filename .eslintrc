{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "rules": {
    "semi": ["error", "never"],
    "comma-dangle": ["error", "never"],
    "quote-props": ["error", "consistent-as-needed"],
    "arrow-body-style": [0],
    "arrow-parens": [0],
    "object-shorthand": [0],
    "global-require": [0],
    "no-underscore-dangle": [0],
    "no-mixed-operators": [0],
    "consistent-return": [0],
    "no-restricted-syntax": [0],
    "guard-for-in": [0],
    "no-await-in-loop": [0],
    "no-shadow": [0],
    "prefer-template": [0],
    "space-infix-ops": [0],
    "class-methods-use-this": [0],
    "no-param-reassign": [0],
    "no-use-before-define": [0],
    "max-len": [0],
    "func-names": [0],
    "import/no-dynamic-require": [0],
    "import/prefer-default-export": [0],
    "no-else-return": [0]
  }
}